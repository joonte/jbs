			<HostingCPUUsage>
				<Name>Учёт используемого процессорного времени пользователями хостинга</Name>
				<Params type="array"/>
				<IsActive>0</IsActive>
				<ExecuteTime>10:00</ExecuteTime>
				<ExecuteDays>1</ExecuteDays>
				<!-- за какой период считаем среднее значение и смотрим превышения -->
				<PeriodToLock>7</PeriodToLock>
				<!-- блокировать превышающих -->
				<LockOverlimits>0</LockOverlimits>
				<!-- автоматически разблокировать залоченных -->
				<UnLockOverlimits>1</UnLockOverlimits>
				<UnLockOverlimitsTime>22:15</UnLockOverlimitsTime>
				<!-- уведомлять превышающих -->
				<NotifyOverlimits>1</NotifyOverlimits>
				<!-- множитель, с которого начинаются уведомления
	(т.е. у юзера лимит 2%, множитель 1.5, значит уведомлять его начнёт только с 3%) -->
				<NotifyRatio>1.5</NotifyRatio>
				<!-- по каким дням блокировать -->
				<LockDays>1</LockDays>
				<!-- множитель, с которого начинаются блокировки
	(т.е. у юзера лимит 2%, множитель 2.0, значит залочит его после DaysOverlimits
	дней превышения, случившихся за PeriodToLock, и если в эти дни у него было от 4% и выше) -->
				<LockRatio>2.0</LockRatio>
				<!-- начиная с какого процента лочить юзеров
	(на мелких тарифах, мелких сайтах, мелких лимитах - лучше вообще не лочить - говна много, эффекта мало) -->
				<LockBeginFrom>2</LockBeginFrom>
				<LockNotifyFrom>1</LockNotifyFrom>
				<CreateTicket>1</CreateTicket>
				<IsEvent>0</IsEvent>
			</HostingCPUUsage>
