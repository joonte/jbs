		<jCountry>
			<Title>Юридический адрес</Title>
			<Check>Country</Check>
			<Comment>Страна юридического адреса</Comment>
			<Example>Россия</Example>
			<Type>Select</Type>
			<Options>
				<AU>Австралия</AU>
				<AT>Австрия</AT>
				<AZ>Азербайджан</AZ>
				<AM>Армения</AM>
				<BY>Беларусь</BY>
				<BG>Болгария</BG>
				<GB>Великобритания</GB>
				<HU>Венгрия</HU>
				<DE>Германия</DE>
				<GE>Грузия</GE>
				<IL>Израиль</IL>
