		<jAddress>
			<Check>Address</Check>
			<Comment>Улица, дом юридического адреса</Comment>
			<Example>Ленина, д. 51, кв. 7</Example>
			<Type>TextArea</Type>
			<Attribs>
				<rows>3</rows>
				<onblur>var Form = document.forms.ProfileEditForm;if(!Form.pAddress.value) Form.pAddress.value = Form.jAddress.value;</onblur>
			</Attribs>
			<IsDuty>1</IsDuty>
			<Value/>
