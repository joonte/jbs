<XML>
	<!-- *********************************************************************** -->
	<ProfileName>ИП "%CompanyName%"</ProfileName>
	<!-- *********************************************************************** -->
	<Attribs>
		<CompanyName>
			<Title>Регистрационные данные</Title>
			<Check>CompanyName</Check>
			<Comment>Наименование ИП (без кавычек, только ФИО)</Comment>
			<Example>Иванов Иван Иванович</Example>
			<Type>TextArea</Type>
			<Attribs>
				<rows>3</rows>
			</Attribs>
			<IsDuty>1</IsDuty>
			<Value/>
		</CompanyName>
		<!-- ********************************************************************** -->
		<Inn>
			<Check>Inn</Check>
			<Comment>ИНН</Comment>
			<Example>770110725987</Example>
			<Type>Input</Type>
			<Attribs>
				<type>text</type>
			</Attribs>
			<IsDuty>1</IsDuty>
			<Value/>
		</Inn>
		<!-- ********************************************************************** -->
		<GRNIP>
			<Check>/^[0-9]{15}$/u</Check>
			<Comment>ГРНИП</Comment>
			<Example>554555447455667</Example>
			<Type>Input</Type>
			<Attribs>
				<type>text</type>
			</Attribs>
			<IsDuty>1</IsDuty>
			<Value/>
		</GRNIP>
		<!-- ********************************************************************** -->
		<OGRNIP>
			<Check>/^[0-9]{15}$/u</Check>
			<Comment>ОГРНИП</Comment>
			<Example>455756445664455</Example>
			<Type>Input</Type>
			<Attribs>
				<type>text</type>
			</Attribs>
			<IsDuty>1</IsDuty>
			<Value/>
		</OGRNIP>
		<!-- ********************************************************************** -->
		<GrnipDate>
			<Check>/^[0-9]{2}\.[0-9]{2}\.[0-9]{4}$/u</Check>
			<Comment>Дата постановки на учёт</Comment>
			<Example>12.03.2005</Example>
			<Type>Input</Type>
			<Attribs>
				<type>text</type>
			</Attribs>
			<IsDuty>1</IsDuty>
			<Value/>
		</GrnipDate>
		<!-- ********************************************************************** -->
		<dSourname>
			<Title>Директор</Title>
			<Check>Sourname</Check>
			<Comment>Фамилия директора</Comment>
			<Example>Иванов</Example>
			<Type>Input</Type>
			<Attribs>
				<type>text</type>
				<onblur>value = FirstCapitalLetter(value); value = DeleteSpace(value);</onblur>
			</Attribs>
			<IsDuty>1</IsDuty>
			<Value/>
		</dSourname>
		<!-- ********************************************************************** -->
		<dName>
			<Check>Name</Check>
			<Comment>Имя директора</Comment>
			<Example>Петр</Example>
