<?xml version="1.0" encoding="utf-8"?>
<!-- @author Бреславский А.В. (Joonte Ltd.) -->
<XML>
	<!-- *********************************************************************** -->
	<ProfileName>%Name%</ProfileName>
	<!-- *********************************************************************** -->
	<Attribs>
		<Name>
			<Check>Char</Check>
			<Comment>Имя</Comment>
			<Example>Joonte</Example>
			<Type>Input</Type>
			<Attribs>
				<type>text</type>
			</Attribs>
			<IsDuty>1</IsDuty>
			<Value/>
		</Name>
		<!-- ********************************************************************** -->
		<SiteURL>
			<Check>Url</Check>
			<Comment>Адрес Вашего сайта</Comment>
			<Example>https://www.joonte.com</Example>
			<Type>Input</Type>
			<Attribs>
				<type>text</type>
			</Attribs>
			<IsDuty>1</IsDuty>
			<Value/>
		</SiteURL>
		<!-- ********************************************************************** -->
		<IconURL>
			<Check>Url</Check>
			<Comment>Адрес Вашей иконки (200x150)</Comment>
			<Example>https://www.joonte.com/Icon.png</Example>
			<Type>Input</Type>
			<Attribs>
				<type>text</type>
			</Attribs>
			<IsDuty>1</IsDuty>
			<Value/>
		</IconURL>
		<!-- ********************************************************************** -->
		<Comment>
			<Check>Text</Check>
			<Comment>Краткое описание деятельности компании (до 1000 символов)</Comment>
			<Example>Сфера деятельности, отличительные особенности ...</Example>
			<Type>TextArea</Type>
			<Attribs>
				<rows>5</rows>
			</Attribs>
			<IsDuty>1</IsDuty>
			<Value/>
		</Comment>
	</Attribs>
	<!-- *********************************************************************** -->
</XML>
